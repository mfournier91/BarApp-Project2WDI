<% @menu_item.errors.full_messages.each do |message| %>
<p>
  <%= message %>
</p>
<% end %>

<h2>Create new menu item</h2>
<%= form_for [@bar, @happyhour, @menu_item] do |f|%>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :price %>: $
<%= f.text_field :price %>

<%= f.submit %>
<% end %>
